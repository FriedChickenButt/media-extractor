{
    "name": "Getting Started Example",
    "version": "1.0",
    "description": "Youtube True PiP",
    "author": "FriedChickenButt",
    "background": {
        "scripts": ["donothing.js"],
        "persistent": false
    },
    "permissions": [
        "tabs",
        "http://*/*",
