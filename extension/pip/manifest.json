{
    "name": "Youtube PIP",
    "version": "1.0",
    "description": "Youtube True PiP",
    "author": "FriedChickenButt",
    "background": {
        "scripts": ["donothing.js"],
        "persistent": false
    },
    "icons": { "16": "icon16.png",
        "48": "icon48.png",
       "128": "icon128.png" },
    "permissions": [
        "tabs",
        "http://*/*",
        "https://*/*"
    ],
    "content_scripts": [
        {
          "matches": ["https://*.youtube.com/watch*"],
          "js": ["background.js"]
        }
    ],
    "manifest_version": 2
}